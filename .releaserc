{
  "branches": ["master"],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    ["@semantic-release/github", {
      "successComment": "This pull request will trigger a new release.",
      "successCommentCount": 0,
      "failureComment": "This pull request cannot trigger a new release."
    }]
  ]
}