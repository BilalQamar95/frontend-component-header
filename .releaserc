{
  "branch": "pull_request",
  "tagFormat": "v${version}",
  "verifyConditions": [
    "@semantic-release/npm",
    ["@semantic-release/github", {
      "onSuccess": "release"
    }]
  ],
  "analyzeCommits": "@semantic-release/commit-analyzer",
  "generateNotes": "@semantic-release/release-notes-generator",
  "prepare": [
    "@semantic-release/npm",
    ["@semantic-release/github", {
      "assets": {
        "path": "dist/*"
      }
    }]
  ],
  "publish": [
    "@semantic-release/npm",
    ["@semantic-release/github", {
      "assets": {
        "path": "dist/*"
      }
    }]
  ],
  "success": [],
  "fail": []
}